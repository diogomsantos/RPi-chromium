{
  "manifest_version": 2,
  "name": "RPi-youtube",
  "description": "This extension runs youtube videos in chromium through omxplayer allowing a much smoother viewing",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["spin.js","get_url.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "nativeMessaging"
  ]
}
